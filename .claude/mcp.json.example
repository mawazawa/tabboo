{
  "mcpServers": {
    "memory": {
      "command": "/opt/homebrew/bin/pnpm",
      "args": [
        "dlx",
        "@modelcontextprotocol/server-memory"
      ],
      "env": {
        "MEMORY_FILE_PATH": "${MEMORY_FILE_PATH}"
      }
    },
    "neo4j-alanse": {
      "command": "/opt/homebrew/bin/pnpm",
      "args": [
        "dlx",
        "@alanse/mcp-neo4j-server@latest"
      ],
      "env": {
        "NEO4J_URI": "${NEO4J_URI}",
        "NEO4J_USERNAME": "${NEO4J_USERNAME}",
        "NEO4J_PASSWORD": "${NEO4J_PASSWORD}",
        "NEO4J_DATABASE": "${NEO4J_DATABASE:-neo4j}"
      }
    },
    "linear": {
      "command": "/opt/homebrew/bin/pnpm",
      "args": [
        "dlx",
        "mcp-remote",
        "https://mcp.linear.app/sse"
      ]
    },
    "perplexity": {
      "command": "/opt/homebrew/bin/pnpm",
      "args": [
        "dlx",
        "server-perplexity-ask"
      ],
      "env": {
        "PERPLEXITY_API_KEY": "${PERPLEXITY_API_KEY}"
      }
    },
    "github": {
      "command": "/opt/homebrew/bin/pnpm",
      "args": [
        "dlx",
        "@modelcontextprotocol/server-github@latest"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_PERSONAL_ACCESS_TOKEN}"
      }
    },
    "supabase": {
      "url": "https://mcp.supabase.com/mcp?project_ref=${SUPABASE_PROJECT_REF}"
    },
    "playwright": {
      "command": "/opt/homebrew/bin/pnpm",
      "args": [
        "dlx",
        "@playwright/mcp@latest",
        "--isolated"
      ]
    },
    "stripe": {
      "url": "https://mcp.stripe.com"
    },
    "vercel": {
      "url": "https://mcp.vercel.com"
    },
    "context7": {
      "command": "/opt/homebrew/bin/pnpm",
      "args": [
        "dlx",
        "@upstash/context7-mcp@latest"
      ]
    },
    "notion": {
      "command": "/opt/homebrew/bin/pnpm",
      "args": [
        "dlx",
        "@notionhq/notion-mcp-server"
      ],
      "env": {
        "NOTION_API_KEY": "${NOTION_API_KEY}"
      }
    },
    "bigquery": {
      "command": "/opt/homebrew/bin/pnpm",
      "args": [
        "dlx",
        "@google-cloud/mcp-server-bigquery@latest"
      ],
      "env": {
        "GOOGLE_CLOUD_PROJECT": "${GOOGLE_CLOUD_PROJECT}",
        "GOOGLE_APPLICATION_CREDENTIALS": "${GOOGLE_APPLICATION_CREDENTIALS}"
      }
    },
    "google-maps-platform": {
      "command": "/opt/homebrew/bin/pnpm",
      "args": [
        "dlx",
        "@googlemaps/code-assist-mcp@latest"
      ],
      "env": {
        "GOOGLE_MAPS_API_KEY": "${GOOGLE_MAPS_API_KEY}"
      }
    },
    "firebase": {
      "command": "/opt/homebrew/bin/pnpm",
      "args": [
        "dlx",
        "firebase-tools@latest",
        "mcp"
      ],
      "env": {
        "FIREBASE_PROJECT": "${FIREBASE_PROJECT}"
      }
    },
    "filesystem": {
      "command": "/opt/homebrew/bin/pnpm",
      "args": [
        "dlx",
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "${PROJECT_ROOT}",
        "${CODE_ROOT}",
        "${CLAUDE_ROOT}"
      ]
    },
    "sequential-thinking": {
      "command": "/opt/homebrew/bin/pnpm",
      "args": [
        "dlx",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    },
    "git-official": {
      "command": "/opt/homebrew/bin/pnpm",
      "args": [
        "dlx",
        "-y",
        "@modelcontextprotocol/server-git",
        "--repository",
        "${PROJECT_ROOT}"
      ]
    },
    "desktop-commander": {
      "command": "/opt/homebrew/bin/pnpm",
      "args": [
        "dlx",
        "@wonderwhy-er/desktop-commander"
      ]
    },
    "npm-helper": {
      "command": "/opt/homebrew/bin/pnpm",
      "args": [
        "dlx",
        "@pinkpixel/npm-helper-mcp"
      ]
    },
    "package-version": {
      "command": "/opt/homebrew/bin/pnpm",
      "args": [
        "dlx",
        "mcp-package-version"
      ]
    },
    "@magicuidesign/mcp": {
      "command": "/opt/homebrew/bin/pnpm",
      "args": [
        "dlx",
        "@magicuidesign/mcp@latest"
      ]
    },
    "@21st-dev/magic": {
      "command": "/opt/homebrew/bin/pnpm",
      "args": [
        "dlx",
        "@21st-dev/magic@latest"
      ],
      "env": {
        "API_KEY": "${MAGIC_API_KEY}"
      }
    },
    "redis": {
      "command": "/opt/homebrew/bin/pnpm",
      "args": [
        "dlx",
        "@modelcontextprotocol/server-redis"
      ],
      "env": {
        "REDIS_URL": "${REDIS_URL}"
      }
    },
    "exa": {
      "command": "/opt/homebrew/bin/pnpm",
      "args": [
        "dlx",
        "exa-mcp-server"
      ],
      "env": {
        "EXA_API_KEY": "${EXA_API_KEY}"
      }
    },
    "Figma": {
      "url": "https://mcp.figma.com/mcp"
    },
    "knowledge-graph": {
      "type": "sse",
      "url": "${KNOWLEDGE_GRAPH_URL:-http://localhost:27495/mcp/sse}",
      "approvedTools": true
    },
    "github-source-control": {
      "command": "/opt/homebrew/bin/pnpm",
      "args": [
        "dlx",
        "@modelcontextprotocol/server-github@latest"
      ]
    },
    "perplexity-ai-research": {
      "command": "/opt/homebrew/bin/pnpm",
      "args": [
        "dlx",
        "@perplexity-ai/mcp-server"
      ]
    },
    "looker": {
      "command": "/opt/homebrew/bin/pnpm",
      "args": [
        "dlx",
        "@google-cloud/mcp-server-looker@latest"
      ]
    }
  }
}

